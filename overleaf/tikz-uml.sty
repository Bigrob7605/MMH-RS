% tikz-uml.sty - TikZ UML Diagram Package% Provides UML diagram capabilities for LaTeX documents\ProvidesPackage{tikz-uml}[2024/01/17 TikZ UML Diagrams]\RequirePackage{tikz}\usetikzlibrary{calc, shapes, arrows, positioning, decorations.pathmorphing, backgrounds, svg.path}\RequirePackage{xcolor}% UML Diagram Styles\tikzset{    uml class/.style={        rectangle,        draw,        minimum width=2cm,        minimum height=1cm,        align=center,        fill=white    },    uml interface/.style={        rectangle,        draw,        minimum width=2cm,        minimum height=1cm,        align=center,        fill=yellow!20,        dashed    },    uml actor/.style={        rectangle,        draw,        minimum width=1.5cm,        minimum height=0.8cm,        align=center,        fill=blue!10    },    uml lifeline/.style={        rectangle,        draw,        minimum width=0.1cm,        minimum height=8cm,        fill=gray!20    },    uml message/.style={        ->,        thick    },    uml return/.style={        ->,        thick,        dashed    },    uml activation/.style={        rectangle,        draw,        fill=green!30,        minimum width=0.3cm,        minimum height=0.5cm    },    uml note/.style={        rectangle,        draw,        fill=yellow!10,        minimum width=2cm,        minimum height=1cm,        align=left,        font=\small    },    uml package/.style={        rectangle,        draw,        minimum width=3cm,        minimum height=2cm,        align=center,        fill=white    },    uml component/.style={        rectangle,        draw,        minimum width=2cm,        minimum height=1cm,        align=center,        fill=green!10    },    uml node/.style={        rectangle,        draw,        minimum width=2cm,        minimum height=1cm,        align=center,        fill=orange!10    }}% Sequence Diagram Environment\newenvironment{sequencediagram}[1][]{    \begin{tikzpicture}[        node distance=2cm,        #1    ]}{    \end{tikzpicture}}% Class Diagram Environment\newenvironment{classdiagram}[1][]{    \begin{tikzpicture}[        node distance=3cm,        #1    ]}{    \end{tikzpicture}}% Use Case Diagram Environment\newenvironment{usecasediagram}[1][]{    \begin{tikzpicture}[        node distance=2.5cm,        #1    ]}{    \end{tikzpicture}}% Component Diagram Environment\newenvironment{componentdiagram}[1][]{    \begin{tikzpicture}[        node distance=3cm,        #1    ]}{    \end{tikzpicture}}% Activity Diagram Environment\newenvironment{activitydiagram}[1][]{    \begin{tikzpicture}[        node distance=2cm,        #1    ]}{    \end{tikzpicture}}% UML Commands\newcommand{\umlclass}[3][]{    \node[uml class, #1] at (#2) {#3};}\newcommand{\umlinterface}[3][]{    \node[uml interface, #1] at (#2) {#3};}\newcommand{\umlactor}[3][]{    \node[uml actor, #1] at (#2) {#3};}\newcommand{\umllifeline}[3][]{    \node[uml lifeline, #1] at (#2) {#3};}\newcommand{\umlmessage}[4][]{    \draw[uml message, #1] (#2) -- (#3);    \node[above] at ($(#2)!.5!(#3)$) {#4};}\newcommand{\umlreturn}[4][]{    \draw[uml return, #1] (#2) -- (#3);    \node[below] at ($(#2)!.5!(#3)$) {#4};}\newcommand{\umlactivation}[3][]{    \node[uml activation, #1] at (#2) {#3};}\newcommand{\umlnote}[3][]{    \node[uml note, #1] at (#2) {#3};}\newcommand{\umlpackage}[3][]{    \node[uml package, #1] at (#2) {#3};}\newcommand{\umlcomponent}[3][]{    \node[uml component, #1] at (#2) {#3};}\newcommand{\umlnode}[3][]{    \node[uml node, #1] at (#2) {#3};}% Sequence Diagram Specific Commands\newcommand{\umlsequence}[5][]{    \umlmessage[#1]{#2}{#3}{#4}}\newcommand{\umlreturnmsg}[5][]{    \umlreturn[#1]{#2}{#3}{#4}}% Class Diagram Specific Commands\newcommand{\umlattribute}[2]{    \node[below] at (#1) {#2}}\newcommand{\umlmethod}[2]{    \node[below] at (#1) {#2}}% Inheritance and Association Commands\newcommand{\umlinheritance}[2]{    \draw[uml message, -triangle 45] (#1) -- (#2);}\newcommand{\umlassociation}[2]{    \draw[uml message] (#1) -- (#2);}\newcommand{\umlcomposition}[2]{    \draw[->, thick, diamond-] (#1) -- (#2)    \node[above] at ($(#1)!.5!(#2)$) {composes}}\newcommand{\umlaggregation}[2]{    \draw[->, thick, diamond] (#1) -- (#2)    \node[above] at ($(#1)!.5!(#2)$) {aggregates}}% Use Case Specific Commands\newcommand{\umlusecase}[3][]{    \node[ellipse, draw, minimum width=2cm, minimum height=1cm, align=center, #1] at (#2) {#3}}\newcommand{\umlsystem}[3][]{    \node[rectangle, draw, minimum width=6cm, minimum height=4cm, align=center, #1] at (#2) {#3}}% Activity Diagram Commands\newcommand{\umlstart}[3][]{    \node[circle, draw, fill=black, minimum size=0.5cm, #1] at (#2) {#3}}\newcommand{\umlend}[3][]{    \node[circle, draw, fill=black, minimum size=0.5cm, #1] at (#2) {#3}}\newcommand{\umlactivity}[3][]{    \node[rectangle, draw, rounded corners, minimum width=2cm, minimum height=1cm, align=center, #1] at (#2) {#3}}\newcommand{\umldecision}[3][]{    \node[diamond, draw, minimum width=2cm, minimum height=1cm, align=center, #1] at (#2) {#3}}% Component Diagram Commands\newcommand{\umlport}[3][]{    \node[rectangle, draw, minimum width=0.5cm, minimum height=0.5cm, align=center, #1] at (#2) {#3}}\newcommand{\umlconnector}[2]{    \draw[uml message, dashed] (#1) -- (#2);}% Package and Namespace Commands\newcommand{\umlnamespace}[3][]{    \node[rectangle, draw, dashed, minimum width=3cm, minimum height=2cm, align=center, #1] at (#2) {#3}}% Stereotype Commands\newcommand{\umlstereotype}[2]{    \node[above, font=\small\itshape] at (#1) {<<#2>>}}% Note and Constraint Commands\newcommand{\umlconstraint}[3][]{    \node[rectangle, draw, fill=yellow!10, minimum width=2cm, minimum height=1cm, align=left, font=\small, #1] at (#2) {#3}}% State Machine Commands\newcommand{\umlstate}[3][]{    \node[rectangle, draw, rounded corners, minimum width=2cm, minimum height=1cm, align=center, #1] at (#2) {#3}}\newcommand{\umlinitial}[3][]{    \node[circle, draw, fill=black, minimum size=0.3cm, #1] at (#2) {#3}}\newcommand{\umlfinal}[3][]{    \node[circle, draw, fill=black, minimum size=0.3cm, #1] at (#2) {#3}}% Transition Commands\newcommand{\umltransition}[2]{    \draw[uml message, ->] (#1) -- (#2);}\newcommand{\umltransitionlabel}[4][]{    \draw[->, thick, #1] (#2) -- (#3)    \node[above] at ($(#2)!.5!(#3)$) {#4}}% Deployment Diagram Commands\newcommand{\umlartifact}[3][]{    \node[rectangle, draw, minimum width=2cm, minimum height=1cm, align=center, #1] at (#2) {#3}}% Communication Diagram Commands\newcommand{\umlobject}[3][]{    \node[rectangle, draw, minimum width=2cm, minimum height=1cm, align=center, #1] at (#2) {#3}}\newcommand{\umllink}[3][]{    \draw[<->, thick, #1] (#2) -- (#3)}% Timing Diagram Commands\newcommand{\umltiming}[3][]{    \node[rectangle, draw, minimum width=0.5cm, minimum height=8cm, align=center, #1] at (#2) {#3}}\newcommand{\umlsignal}[3][]{    \draw[->, thick, #1] (#2) -- (#3)}% Profile and Extension Commands\newcommand{\umlprofile}[3][]{    \node[rectangle, draw, fill=purple!10, minimum width=3cm, minimum height=2cm, align=center, #1] at (#2) {#3}}\newcommand{\umlextension}[2]{    \draw[->, thick, dashed] (#1) -- (#2)    \node[above] at ($(#1)!.5!(#2)$) {extends}}% Metaclass Commands\newcommand{\umlmetaclass}[3][]{    \node[rectangle, draw, fill=red!10, minimum width=2cm, minimum height=1cm, align=center, #1] at (#2) {#3}}% Template Commands\newcommand{\umltemplate}[3][]{    \node[rectangle, draw, fill=orange!10, minimum width=2cm, minimum height=1cm, align=center, #1] at (#2) {#3}}% Utility Commands\newcommand{\umlcomment}[3][]{    \node[right, font=\small, #1] at (#2) {#3}}\newcommand{\umlgroup}[3][]{    \node[rectangle, draw, dashed, minimum width=4cm, minimum height=3cm, align=center, #1] at (#2) {#3}}\endinput 